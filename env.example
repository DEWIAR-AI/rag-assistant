APP_NAME=RAG Restaurant Management System
APP_VERSION=1.0.0
DEBUG=true
ENVIRONMENT=demo

# OpenAI API
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# Gemini API
LLM_PROVIDER=gemini
GEMINI_API_KEY=your_google_api_key_here
GEMINI_MODEL=

# PostgreSQL Database
DATABASE_URL=postgresql://postgres.login:password@aws-0-eu-north-1.pooler.supabase.com:6543/postgres
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=rag_database
DATABASE_USER=username
DATABASE_PASSWORD=password

# Supabase Configuration
SUPABASE_URL=https://example.supabase.co
SUPABASE_SERVICE_KEY=
SUPABASE_BUCKET=rag-files

# Qdrant Vector Database (Cloud)
QDRANT_URL=
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=restaurant_documents
QDRANT_VECTOR_SIZE=1536

# JWT Settings
JWT_SECRET_KEY=your_super_secret_jwt_key_here_make_it_long_and_random
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# File Upload Settings
UPLOAD_DIR=uploads
MAX_FILE_SIZE=10485760  # 10MB in bytes
# –ü–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ–º—ã–µ —Ñ–æ—Ä–º–∞—Ç—ã: PDF, DOCX, TXT, XLSX, XLS, PPT, PPTX, MD, CSV, RTF
ALLOWED_FILE_TYPES=pdf,docx,txt,xlsx,xls,ppt,pptx,md,csv,rtf
TEMP_DIR=temp

# Server Settings
HOST=0.0.0.0
PORT=8000
WORKERS=4
RELOAD=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# üîí Security
# CORS_ORIGINS –º–æ–∂–Ω–æ –∑–∞–¥–∞—Ç—å –∫–∞–∫ JSON –º–∞—Å—Å–∏–≤ –∏–ª–∏ —Å—Ç—Ä–æ–∫—É, —Ä–∞–∑–¥–µ–ª–µ–Ω–Ω—É—é –∑–∞–ø—è—Ç—ã–º–∏
# –ü—Ä–∏–º–µ—Ä—ã:
# CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080", "https://your-domain.com"]
